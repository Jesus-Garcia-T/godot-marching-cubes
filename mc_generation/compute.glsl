float density(in vec3 pos){
    return texture(input_tex, pos).r * 2.0 - 1;
}

uint get_case(in ivec3 pos){
    uint v = (density(pos) > 0) ? 1 : 0;
    v |= uint(density(pos + ivec3(0,0,1)) > 0) << 1;
    v |= uint(density(pos + ivec3(1,0,1)) > 0) << 2;
    v |= uint(density(pos + ivec3(1,0,0)) > 0) << 3;
    v |= uint(density(pos + ivec3(0,1,0)) > 0) << 4;
    v |= uint(density(pos + ivec3(0,1,1)) > 0) << 5;
    v |= uint(density(pos + ivec3(1,1,1)) > 0) << 6;
    v |= uint(density(pos + ivec3(1,1,0)) > 0) << 7;

    return v;
}

vec3 interpolate(in ivec3 a, in ivec3 b){
    float d_a = density(a);
    float d_b = density(b);
    float t = (-d_a) / (d_b - d_a);
    vec3 f_a = vec3(a), f_b = vec3(b);
    return f_a + (t * (f_b - f_a));
    //return (f_a + f_b) / 2;
}

vec3 sobelNormal(in vec3 pos){
    float gx = 0, gy = 0, gz = 0, dense;

    for(int dz=0; dz <=2; dz++)
        for(int dy=0; dy <=2; dy++)
            for(int dx=0; dx <=2; dx++){
                dense = density(pos + vec3(dx-1,dy-1,dz-1));

                gx += dense * sobelX[dz][dy][dx];
                gy += dense * sobelY[dz][dy][dx];
                gz += dense * sobelZ[dz][dy][dx];
            }

    return (-normalize(vec3(gx,gy,gz)));
}

vec3 calculateNormal(in vec3 v){
    float x = v.x, y = v.y, z = v.z;

    float dx = density(vec3(x+1,y,z)) - density(vec3(x-1,y,z));
    float dy = density(vec3(x,y+1,z)) - density(vec3(x,y-1,z));
    float dz = density(vec3(x,y,z+1)) - density(vec3(x,y,z-1));

    return (-normalize(vec3(dx,dy,dz)));
}

void march_cube(in ivec3 pos){
    uint id = get_case(pos);
    int tri[16] = tris[id];
    int i = 0;
    int e = tri[i];

    ivec2 vert;
    ivec3 v1, v2;
    vec3 vf, vn;
    while(e >= 0){

        uint write_index = atomicAdd(tri_count, 1) * 3;

        //////1
        vert = edge[e];

        v1 = pos + verts[vert[0]];
        v2 = pos + verts[vert[1]];

        vf = interpolate(v1, v2);
        vn = sobelNormal(vf);

        vertices[write_index] = vf;
        normales[write_index] = vn;

        i += 1;
        e = tri[i];
        
        //////2
        vert = edge[e];

        v1 = pos + verts[vert[0]];
        v2 = pos + verts[vert[1]];

        vf = interpolate(v1, v2);
        vn = sobelNormal(vf);

        vertices[write_index + 1] = vf;
        normales[write_index + 1] = vn;

        i += 1;
        e = tri[i];
        
        //////3
        vert = edge[e];

        v1 = pos + verts[vert[0]];
        v2 = pos + verts[vert[1]];

        vf = interpolate(v1, v2);
        vn = sobelNormal(vf);

        vertices[write_index + 2] = vf;
        normales[write_index + 2] = vn;

        i += 1;
        e = tri[i];
    }
}

// The code we want to execute in each invocation
void main() {
    // gl_GlobalInvocationID.x uniquely identifies this invocation across all work groups
    ivec3 pos = ivec3(gl_GlobalInvocationID.xyz);

    if (get_case(pos) >= 128){
        atomicAdd(voxel_count,1);
    }

    march_cube(pos);
}